<Window x:Class="Entorno_visual.WindowUpload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Entorno_visual"
        mc:Ignorable="d"
        Title="Upload Your Content" Height="450" Width="800">
    <Grid>
        <Button x:Name="back_button" Content="Go Back" HorizontalAlignment="Left" Margin="33,372,0,0" VerticalAlignment="Top" Width="75" Click="back_button_Click"/>
        <Button x:Name="exit_button" Content="Exit" HorizontalAlignment="Left" Margin="678,372,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="Release Name:" HorizontalAlignment="Left" Margin="33,27,0,0" VerticalAlignment="Top" Width="117"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="33,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
